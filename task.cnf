[sysbench]
THREAD_NUM_LIST=128
STATS_CYCLE=5
DB_NAME=sbtest
PWD=intel123
WORK_LOAD=/usr/share/sysbench/oltp_read_write.lua
TABLE_NUM=10
ROWS=1000000
#DYNAMIC_BUFFER_POOL_SIZE=True
#BUFFER_CHANGE_STEP=0.05
#EXTRA_MEM_LIST=10,20

[db_bench_env]
installDir=/home/john/rocksdb-runing
#LD_LIBRARY_PATH=/home/john/test/gflags/.libs/:/home/john/test/zstd/lib
#db_bench_path=/home/john/test/rocksdb/db_bench

[db_bench]
db=/mnt/rocksdb-tlc
num_levels=6
key_size=64
value_size=1024
block_size=4096
cache_size=$((8 * 1024 * 1024 * 1024))
cache_numshardbits=6
compression_type=none
compression_ratio=0.5
hard_rate_limit=2
rate_limit_delay_max_milliseconds=1000000
write_buffer_size=$((128 * 1024 * 1024))
max_write_buffer_number=2
target_file_size_base=$((128 * 1024 * 1024))
max_bytes_for_level_base=$((1024 * 1024 * 1024)) 
max_bytes_for_level_multiplier=10
sync=0
verify_checksum=1
delete_obsolete_files_period_micros=$((60 * 1024 * 1024))
statistics=1
stats_per_interval=5
stats_interval=10000000
histogram=1
memtablerep=skip_list
bloom_bits=10
num_multi_db=1
rate_limiter_bytes_per_sec=0
open_files=0
max_background_compactions=32
max_background_flushes=32
level0_file_num_compaction_trigger=8
level0_slowdown_writes_trigger=16
level0_stop_writes_trigger=24
benchmarks=readwhilewriting
use_existing_db=1
num=1000000000
threads=4
duration=12000

# read,write,randread,randwrite,randrw
[fio_global]
rwList=randrw
runtime=180

[read]
numjobs=1
iodepth=8
bs=128k

[write]
numjobs=1,4
iodepth=8
bs=64k,128k

[randwrite]
numjobs=8
iodepth=8
bs=4k,8k

[randread]
numjobs=8
iodepth=8,16,32
bs=4k,8k

[randrw]
numjobs=8
iodepth=8,32
bs=4k,8k
rwmixread=70
random_distribution=zipf:1.2
bssplit=4k/18:128k/27:512k/,4k/:512k/19:1m/21:2m/26:4m/26
