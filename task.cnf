[sysbench]
THREAD_NUM_LIST=100
STATS_CYCLE=5 
DB_NAME=sbtest
PWD=intel123
DYNAMIC_BUFFER_POOL_SIZE=True
BUFFER_CHANGE_STEP=0.1
WORK_LOAD=/usr/share/sysbench/oltp_read_write.lua
EXTRA_MEM_LIST=8

#If you want to try multiple threads
#THREAD_NUM_LIST=100,128
#DYNAMIC_BUFFER_POOL_SIZE=False

# read,write,randread,randwrite,randrw
[fio_global]
#rwList=read,randread,write,randwrite
rwList=read,randread,write,randwrite
time=300

[read]
numjoblist=1
iodepthlist=8
bslist=128k

[write]
numjoblist=1
bslist=64k

[randwrite]
numjoblist=8
bslist=4k

[randread]
numjoblist=8
bslist=4k
